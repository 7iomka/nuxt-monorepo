{
  "name": "auth-vue-fastify",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\"",
    "typecheck": "turbo run typecheck",
    "preinstall": "npx only-allow pnpm",
    "check-updates": "turbo check-updates",
    "lint:monorepo": "pnpm sherif"
  },
  "devDependencies": {
    "@types/node": "^20.17.4",
    "prettier": "3.3.3",
    "sherif": "1.0.1",
    "turbo": "^2.2.3",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9"
  }
}
